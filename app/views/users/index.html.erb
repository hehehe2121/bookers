<div class="container">
	<div class="low">
		<div class="col-xs-3">
			<div class="profile-container">
				<%= render 'users/list', user: current_user %>
				<%= render 'bookers/list', booker: @booker %>
			</div>
		</div>
		<div class="col-xs-9">
			<h2>Users</h2>
				<table class="table table-striped">
					<thead>
						<tr>
							<th>image</th>
							<th>name</th>
							<th></th>
						</tr>
					</thead>
					<tbody>
						<% @users.each do |user| %>
						<tr>
							<td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', class: "img-circle profile-img", fallback: "no_image.jpg" %></td>
							<td><%= user.name %></td>
							<td><%= link_to 'show', user_path(user) %></td>
						</tr>
						<% end %>
					</tbody>
				</table>
		</div>
	</div>
</div>