<div class="new-book-box">
	<h2>New book</h2>
		<%= form_for(booker) do |f| %>
		<div class="field">
			<%= f.label :title, "Title" %><br>
			<%= f.text_field :title,class: "book_title" %>
		</div>
		<div class="field">
			<%= f.label :body, "body" %><br>
			<%= f.text_area :body,class: "book_body" %>
		</div>
			<input class="btn btn-primary create-btn" type="submit" value="Create Box">
			<% end %>
</div>

